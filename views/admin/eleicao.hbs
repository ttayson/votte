			<div class="panel panel-container">
			<div class="container">
			
				<div class="card-body">
				<div class="table-responsive">
				<table id="tabelacolaboradores" class="table table-bordered table-striped">
					
					<thead>
					<tr>
					<th scope="col" >Nome</th>
					<th scope="col">Cargo</th>
					<th scope="col">Candidatos</th>
					<th scope="col">Descrição</th>
					<th scope="col">Status</th>
					<th scope="col">Ação</th>
					</tr>

					</thead>
					<tbody>
                  {{#each eleicao}}
                  <tr>
                    
                    <td>{{nome}}</td>
					<td>{{cargo}}</td>
					<td>
					{{#each candidatos}}
					<button type="button" class="btn btn-info btn-sm">{{nome}}</button><br>
					{{else}}
                	{{/each}} 
					</td>
                    <td>{{descricao}}</td>
					<td>{{status}}</td>
					<td>
								<a href="/admin/eleicao/edit/{{id}}">
									<button type="button" id="{{_id}}" onclick="teste('{{id}}')" class="btn btn-primary btn-sm">
									<span class="fa fa-pencil">&nbsp;</span>
									</button>
								</a>

								<span class="btn btn-danger btn-sm" onclick="deletar('{{id}}')">
									<input type="hidden" id="typedel-{{id}}" name="id" value="{{id}}">
									<i class="fa fa fa-trash-o"></i>
								</span>

								<span class="btn btn-success btn-sm" onclick="teste('{{id}}')">
									<input type="hidden" id="typedel-{{id}}" name="id" value="{{id}}">
									<i class="fa fa fa-trash-o"></i>
								</span>

								
					</td>

                  </tr>

                {{else}}
				

                {{/each}} 
                  
                </tbody>

              </table>
