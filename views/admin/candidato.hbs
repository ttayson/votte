			<div class="panel panel-container">
			<div class="container">
			
				<div class="card-body">
				<div class="table-responsive">
				<table id="tabelacolaboradores" class="table table-bordered table-striped">
					
					<thead>
					<tr>
					<th scope="col" >Nome</th>
					<th scope="col">Matrícula</th>
					<th scope="col">Cargo</th>
					<th scope="col">Descrição</th>
					<th scope="col">Ação</th>
					</tr>

					</thead>
					<tbody>
                  {{#each candidato}}
                  <tr>
                    
                    <td>{{nome}}</td>
                    <td>{{matricula}}</td>
                    <td>{{cargo}}</td>
                    <td>{{descricao}}</td>
					<td>
						<div class="flex-container">
							<div class="p-2">
								<a href="/admin/candidato/edit/{{id}}">
									<button type="button" class="btn btn-primary btn-sm">
									<span class="fa fa-cog">&nbsp;</span>
									</button>
								</a>
							</div>
							<div class="p-2">
								<form action="/admin/candidato/del" method="POST">
									<input type="hidden" name="id" value="{{_id}}">
									<button type="submit" class="btn btn-danger btn-sm"><span class="fa fa fa-trash-o">&nbsp;</span></button>
								</form>
							</div>
							<div class="p-2">
								<button type="button" class="btn btn-success btn-sm"><span class="fa fa fa-lock">&nbsp;</span></button>
							</div>
						</div>

					</td>

                  </tr>




                {{else}}
				

                {{/each}} 
                  
                </tbody>

              </table>
